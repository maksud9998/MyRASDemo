<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myrasdemo">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ras_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ras_icon"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.MyRASDemo">
        <activity android:name=".ForgetPassword"></activity>
        <activity android:name=".EmailVerify" />
        <activity android:name=".UploadCarImages" />
        <activity android:name=".ContactUs" />
        <activity android:name=".Register" />
        <activity android:name=".Payment" />
        <activity android:name=".Login" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EditProfile" />
        <activity android:name=".BookingDetails" />
        <activity android:name=".PreviousBooking" />
        <activity android:name=".UploadDocs" />
        <activity android:name=".PrivacyPolicies" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreenActivity"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>